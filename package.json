{
  "name": "fantastic-box-co",
  "version": "0.0.1",
  "description": "Packing app build with React + MobX",
  "readme": "README.md",
  "license": "MIT",
  "authors": [
    "Ivan Dimanov <npm@idimanov.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvanDimanov/FantasticBoxCo.git"
  },
  "scripts": {
    "start": "npm run build && http-server -o",
    "start-dashboard": "cross-env NODE_ENV=local cross-env dashboard=true webpack-dashboard webpack-dev-server --content-base=./public --inline --progress --colors --hot",
    "build": "npm i && webpack",
    "lint": "standard",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "mocha ./tests/unit/.setup.js ./tests/unit/**/*.test.js"
  },
  "engines": {
    "node": ">=7.0"
  },
  "dependencies": {
    "cross-env": "3.1.3",
    "http-server": "0.13.0",
    "material-ui": "0.16.1",
    "mobx": "2.6.1",
    "mobx-react": "3.5.8",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "react-tap-event-plugin": "1.0.0",
    "rxjs": "5.0.0-rc.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "enzyme": "2.6.0",
    "jsdom": "9.8.3",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.1",
    "standard": "8.5.0",
    "webpack": "1.13.3",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2"
  }
}
